{
  "name": "agentic-course-page",
  "version": "1.0.0",
  "description": "AI-Native Agentic Coding Bootcamp - Enterprise Software Development Training landing page built with HTML5, SCSS, and modern web technologies",
  "main": "index.html",
  "keywords": [
    "ai-native",
    "agentic-coding",
    "enterprise-training",
    "bootcamp",
    "landing-page",
    "sass",
    "responsive"
  ],
  "homepage": "https://github.com/yourusername/ai-course",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-course.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:css": "sass src/scss/app/styles.scss dist/styles.css --no-source-map --style=compressed",
    "build:legacy": "npm run build:css && cp index.html dist/index.html",
    "start": "npm run dev",
    "watch:css": "sass --watch src/scss/app/styles.scss:dist/styles.css --no-source-map",
    "start:legacy": "concurrently \"npm run watch:css\" \"live-server --open=index.html --watch=index.html,dist/styles.css,src/**/*.html --port=8080\"",
    "clean": "rm -rf dist/* && mkdir -p dist",
    "prebuild": "npm run clean"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^6.1.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "live-server": "^1.2.2",
    "sass": "^1.69.0",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
